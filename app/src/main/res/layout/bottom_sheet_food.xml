<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="173dp"
    android:background="@drawable/bg_white_round">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_sheet_food_const1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/bottom_sheet_food_txt_food_kinds"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="42dp"
            android:text="음식 종류"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="11sp"
            android:textColor="@color/brownish_grey"
            android:lineSpacingExtra="6sp"/>

        <LinearLayout
            android:id="@+id/bottom_sheet_food_linear1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginHorizontal="16dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@id/bottom_sheet_food_txt_food_kinds">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="한식"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:layout_marginRight="10dp"
                android:lineSpacingExtra="6sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="중식"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:layout_marginRight="10dp"
                android:lineSpacingExtra="6sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="일식/회"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:layout_marginRight="10dp"
                android:lineSpacingExtra="6sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="양식"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:layout_marginRight="10dp"
                android:lineSpacingExtra="6sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="아시안식"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:layout_marginRight="10dp"
                android:lineSpacingExtra="6sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="분식"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:layout_marginRight="10dp"
                android:lineSpacingExtra="6sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="치킨"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:layout_marginRight="10dp"
                android:lineSpacingExtra="6sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="피자"
                android:background="@drawable/bg_white_four_round"
                android:textSize="10sp"
                android:textColor="@color/brownish_grey"
                android:lineSpacingExtra="6sp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="16dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@id/bottom_sheet_food_linear1"
            tools:layout_editor_absoluteX="16dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:background="@drawable/bg_white_four_round"
                android:lineSpacingExtra="6sp"
                android:text="고기"
                android:textColor="@color/brownish_grey"
                android:textSize="10sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:background="@drawable/bg_white_four_round"
                android:lineSpacingExtra="6sp"
                android:text="찜/탕"
                android:textColor="@color/brownish_grey"
                android:textSize="10sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:background="@drawable/bg_white_four_round"
                android:lineSpacingExtra="6sp"
                android:text="패스트푸드"
                android:textColor="@color/brownish_grey"
                android:textSize="10sp" />
        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f9f9f9"
            android:layout_marginTop="22dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bottom_sheet_food_const1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="22dp"
            android:text="테마"
            android:textSize="11sp"
            android:textColor="@color/brownish_grey"
            android:lineSpacingExtra="6sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>